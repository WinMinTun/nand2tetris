/**
 * 16-bit bitwise And:
 * for i = 0..15: out[i] = (a[i] and b[i])
 */

CHIP And16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    // Put your code here:
	Nand (a=a[0..15], b=[0..15], out=nand1Out);
	Nand (a=a[0..15], b=b[0..15], out=nand2Out);
	Nand (a=nand1Out[0..15], b=nand2Out[0..15], out=out);
}